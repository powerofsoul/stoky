// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id      String   @id
  email   String?   @unique
  username    String?
  firstName String?
  lastName String?
  picture String?
  aboutMe String?
  location String?
  createdAt DateTime? @default(now())
}